@startuml
!theme materia
Class ProcesoDeElaboracion{
-costo:double
-productos :ArrayList<ProductoDeConfiteria>
-empleados : ArrayList<Empleado>
+ProcesoDeElaboración ()
+ProcesoDeElaboración(costo :double,empleados :ArrayList<Empleado>,productos :ArrayList<ProductoDeConfiteria>)
+getCosto() : double
+setCosto(costo : doble) : void
+getProductos() :ArrayList<ProductoDeConfiteria>
+ setProductos(productos : ArrayList<Producto DeConfiteria>) : void
+getEmpleados() : ArrayList<Empleado>
+setEmpleados(empleados :ArrayList<Empleado>) : void
}
Class Maquinaria{
-estado : string
-tipoDeProceso : string
+Maquinaria()
+Maquinaria(estado : string, tipoDeProceso : string)
+getEstado() : string
+setEstado(estado : string) : void
+getTipoDeProceso() : string
+setTipoDeProceso(tipoDeProceso:string):void
}
Class Operario{
-salario : double
-cargo : string
+Operario()
+Operario(salario : double, cargo : string)
+getSalario() : double
+setSalario(salario : double) : void
+getCargo() : string
+setCargo(cargo : string) : void
}
ProcesoDeElaboracion "1" o--- "1..*" Maquinaria
Maquinaria "1" o--- "1" Operario
@enduml